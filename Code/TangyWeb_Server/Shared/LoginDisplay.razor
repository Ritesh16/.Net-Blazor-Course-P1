@inject IAuthService _authService

<AuthorizeView>
    <Authorized>
        <a>@context.User.Identity.Name</a>
        <a href="/logout">Logout</a>
    </Authorized>
    <NotAuthorized>
        <a href="/login">Login</a>
        <a href="/register">Register</a>
    </NotAuthorized>

</AuthorizeView>

